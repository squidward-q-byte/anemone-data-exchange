@namespace("ade.tabular")

protocol AnomalyDetection {
    import idl "raw.avdl";

    record AnomalyScore {
        float score;
        boolean is_anomaly;
    }

    record AnomalyDetectionRequest {
        ade.tabular.Table table;
    }

    record AnomalyDetectionResponse {
        array<AnomalyScore> anomaly_scores;
    }
}
